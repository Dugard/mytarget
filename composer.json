{
    "name": "dugard/mytarget",
    "type": "library",
    "description": "PHP library for MyTarget API",
    "keywords": [
        "php",
        "myTarget",
		"api",
        "client",
        "ads"
    ],
    "homepage": "https://github.com/Dugard/mytarget-api-client",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ivan Shkuta",
            "email": "shkuta85@gmail.com",
            "homepage": "https://github.com/Dugard",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.0",
        "guzzlehttp/guzzle": ">=6.3 <8.0.0",
        "dugard/mytarget-oauth2": "^0.2.4"
    },
    "require-dev": {
        "codeception/codeception": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "dugard\\myTarget\\api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dugard\\myTarget\\api\\tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./codecept run unit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
